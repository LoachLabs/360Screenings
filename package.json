    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.4.1",
    "docpad-plugin-thumbnails": "~2.1.1",
    "docpad-plugin-ghpages": "~2.4.1",
    "docpad-plugin-cleanurls": "~2.6.0",
    "docpad-plugin-sitemap": "~2.2.0"
  },
  "devDependencies": {
    "docpad-plugin-livereload": "~2.5.3",
    "grunt-cli": "~0.1.11",
    "bower": "~1.2.7",
    "docpad-plugin-services": "~2.5.0"
  },
  "main": "node_modules/docpad/bin/docpad-server",
  "scripts": {
